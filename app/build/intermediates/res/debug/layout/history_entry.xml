<com.numix.calculator_pro.view.HistoryLine xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="12dp" >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/small_display_text_size"
        android:textColor="?history_input"
        android:id="@+id/historyExpr" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/display_text_size"
        android:textColor="?history_result"
        android:gravity="right"
        android:id="@+id/historyResult" />
</com.numix.calculator_pro.view.HistoryLine>
